  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sambhav-Gautam/osmemoryallocator.git"
  },
  "bugs": {
    "url": "https://github.com/Sambhav-Gautam/osmemoryallocator/issues"
  },
  "homepage": "https://github.com/Sambhav-Gautam/osmemoryallocator#readme",
